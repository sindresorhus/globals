{
	"name": "globals",
	"version": "14.0.0",
	"description": "Global identifiers from different JavaScript environments",
	"license": "MIT",
	"repository": "sindresorhus/globals",
	"funding": "https://github.com/sponsors/sindresorhus",
	"author": {
		"name": "Sindre Sorhus",
		"email": "sindresorhus@gmail.com",
		"url": "https://sindresorhus.com"
	},
	"sideEffects": false,
	"engines": {
		"node": ">=18"
	},
	"scripts": {
		"test": "xo && ava && tsd",
		"prepare": "npm run build",
		"update": "run-p \"update:*\"",
		"update:browser": "node scripts/get-browser-globals.mjs",
		"update:builtin": "node scripts/get-builtin-globals.mjs",
		"update:node": "node scripts/get-node-globals.mjs",
		"build": "run-p \"build:*\"",
		"build:data": "node scripts/generate-data.mjs",
		"build:types": "node scripts/generate-types.mjs > index.d.ts"
	},
	"files": [
		"index.js",
		"index.d.ts",
		"globals.json"
	],
	"keywords": [
		"globals",
		"global",
		"identifiers",
		"variables",
		"vars",
		"jshint",
		"eslint",
		"environments"
	],
	"devDependencies": {
		"ava": "^6.1.1",
		"cheerio": "^1.0.0-rc.12",
		"npm-run-all2": "^6.1.2",
		"outdent": "^0.8.0",
		"puppeteer": "^22.1.0",
		"tsd": "^0.30.4",
		"type-fest": "^4.10.2",
		"xo": "^0.57.0"
	},
	"xo": {
		"rules": {
			"unicorn/prefer-module": "off"
		},
		"overrides": [
			{
				"files": [
					"data/*.mjs"
				],
				"rules": {
					"import/no-anonymous-default-export": "off",
					"camelcase": "off",
					"unicorn/filename-case": [
						"error",
						{
							"cases": {
								"camelCase": true,
								"kebabCase": true
							}
						}
					]
				}
			}
		]
	},
	"tsd": {
		"compilerOptions": {
			"resolveJsonModule": true
		}
	}
}
